{
    "name": "lukaskolista/gift",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Lukaskolista\\Gift\\": "src/",
            "Lukaskolista\\Gift\\Framework\\": "framework/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lukaskolista\\Gift\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Łukasz Kołodziejczak"
        }
    ],
    "require": {
        "php": "8.2.*",
        "ext-mongodb": "*",
        "mongodb/mongodb": "^1.16",
        "psr/http-message": "^2.0",
        "guzzlehttp/psr7": "^2.6",
        "symfony/uid": "^7.0",
        "symfony/http-foundation": "^7.0",
        "symfony/routing": "^7.0",
        "symfony/psr-http-message-bridge": "^7.0",
        "symfony/dependency-injection": "^7.0",
        "symfony/config": "^7.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "phpunit/phpunit": "^10.4"
    }
}
